{
    # Description of any external input
    inputs: {
        models: {
            'model1': {
                location: "path/to/my/model.xml"
                language: "urn:sedml:language:sbml"
            },
            'model2': {
                location: "path/to/my/model.xml"
                language: "urn:sedml:language:sbml"
            }
        },
        data: {
            'experiment1': {
                location: "path/to/data.csv"
                format: "http://purl.org/NET/mediatypes/text/csv"
            }
        }
    }

    # All the analyses and calculations.  Processes, essentially.
    tasks: {
        "sim1": {
            _type: timeCourse
            model: "model1"
            initialTime: 0
            outputStartTime: 0
            outputEndTime: 500
            numberOfSteps: 1000
            outputSelections: ["time", "S1", "S2", "compartment"]
        },
        "sim2": {
            _type: timeCourse
            model: "model1"
            initialTime: 0
            outputStartTime: 0
            outputEndTime: 500
            numberOfSteps: 1000
            outputSelections: ["time", "S1", "S2", "compartment"]
        },
        'compare': {
            _type: calculation,
            math: "(tasks:sim1 - tasks:sim2)^2"
        },
        'compare_summary: {
            _type: calculation,
            math: "sum(tasks:compare)"
        }
    },

    # Outputs at the end
    outputs: {
        reports: {
            "sim1Report": {
                outputs: "tasks:sim1"
                filename: "sim1.csv" # Maybe?  Some people don't like this in SED.
                file_format: CSV
            },
            "sim2Report": {
                outputs: "tasks:sim2"
                filename: "sim2.csv" # Maybe?  Some people don't like this in SED.
                file_format: CSV
            },
            "comparisonReport": {
                outputs: "tasks:compare"
                filename: "all_sums_of_squares.csv",
                file_format: "CSV"
            },
            "comparisonSummary": {
                outputs: "tasks:compare_summary",
                # Don't worry about where output goes, just get it to the user somehow.
            }
        }
        plots: {
            "Fig1": {
                label: "Figure 1"
                xAxis: {
                    label: "Time"
                },
                yAxis: {
                    label: "Concentrations"
                }
                curves: {
                    "Sim1_S1": {
                        x: "tasks:sim1:time"
                        y: "tasks:sim1:S1"
                    }
                    "Sim1_S2": {
                        x: "tasks:sim1:time"
                        y: "tasks:sim1:S2"
                    }
                    "Sim2_S1": {
                        x: "tasks:sim1:time"
                        y: "tasks:sim2:S1"
                    }
                    "Sim2_S2": {
                        x: "tasks:sim1:time"
                        y: "tasks:sim2:S2"
                    }
                    "S1_data": {
                        x: "experiment1:time"
                        y: "experiment1:S1"
                        style: "points_only"
                    }
                    "S2_data": {
                        x: "experiment1:time"
                        y: "experiment1:S2"
                        style: "points_only"
                    }
                },
            }
        }
    }
    styles: {
        "points_only": {
            line: False
            markers: True
        }
    }
}
