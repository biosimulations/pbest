{
  "inputs": {
    "models": {
      "model1": {
        "location": "path/to/my/model.xml",
        "language": "urn:sedml:language:sbml"
      }
    },
    "data": {
      "experiment1": {
        "location": "path/to/data.csv",
        "format": "http://purl.org/NET/mediatypes/text/csv"
      }
    }
  },

  "processes": {
    "sim1": {
      "_type": "uniformTimeCourse",
      "model": "model1",
      "initialTime": 0,
      "outputStartTime": 0,
      "outputEndTime": 500,
      "numberOfSteps": 1000,
      "outputVariables": ["time", "S1", "S2", "compartment"]
    },
    "sim2": {
      "_type": "uniformTimeCourse",
      "model": "model1",
      "initialTime": 0,
      "outputStartTime": 0,
      "outputEndTime": 500,
      "numberOfSteps": 1000,
      "outputVariables": ["time", "S1", "S2", "compartment"]
    },
    "sim1_sim2_compare": {
      "_type": "calculation",
      "math": "(processes:sim1 - processes:sim2)^2"
    },
    "sim1_exp_compare": {
      "_type": "calculation",
      "math": "(processes:sim1 - data:experiment1)^2"
    },
    "sim2_exp_compare": {
      "_type": "calculation",
      "math": "(processes:sim2 - data:experiment1)^2"
    },
    "compare_summary": {
      "_type": "calculation",
      "math": "[sum(processes:sim1_sim2_compare), sum(processes:sim1_exp_compare), sum(processes:sim2_exp_compare)]"
    }
  },

  "outputs": {
    "reports": {
      "sim1Report": {
        "outputs": "processes:sim1",
        "filename": "sim1.csv",
        "file_format": "http://purl.org/NET/mediatypes/text/csv"
      },
      "sim2Report": {
        "outputs": "processes:sim2",
        "filename": "sim2.csv",
        "file_format": "http://purl.org/NET/mediatypes/text/csv"
      },
      "comparisonReport": {
        "outputs": "processes:compare",
        "filename": "all_sums_of_squares.csv",
        "file_format": "http://purl.org/NET/mediatypes/text/csv"
      },
      "comparisonSummary": {
        "outputs": "processes:compare_summary"
      }
    },
    "plots": {
      "Fig1": {
        "label": "Figure 1",
        "xAxis": {
          "label": "Time"
        },
        "yAxis": {
          "label": "Concentrations"
        },
        "curves": {
          "Sim1_S1": {
            "x": "processes:sim1:time",
            "y": "processes:sim1:S1"
          },
          "Sim1_S2": {
            "x": "processes:sim1:time",
            "y": "processes:sim1:S2"
          },
          "Sim2_S1": {
            "x": "processes:sim1:time",
            "y": "processes:sim2:S1"
          },
          "Sim2_S2": {
            "x": "processes:sim1:time",
            "y": "processes:sim2:S2"
          },
          "S1_data": {
            "x": "experiment1:time",
            "y": "experiment1:S1",
            "style": "points_only"
          },
          "S2_data": {
            "x": "experiment1:time",
            "y": "experiment1:S2",
            "style": "points_only"
          }
        }
      }
    },
    "styles": {
      "points_only": {
        "line": false,
        "markers": true
      }
    }
  }
}
